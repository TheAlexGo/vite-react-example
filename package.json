{
  "name": "vite-react-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook -o sb-dist",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:tslint": "tsc --noEmit",
    "lint:stylelint": "stylelint '**/*.styl'",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "lint:prettier": "prettier --check **/*.{ts,tsx}",
    "prepare": "husky install",
    "lint:stylelint:fix": "stylelint '**/*.styl' --fix",
    "lint:eslint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:prettier:fix": "prettier --write **/*.{ts,tsx}",
    "watch:ts": "tsc --noEmit --watch"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.5",
    "babel-loader": "^9.1.2",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.7",
    "stylelint": "^15.4.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^6.0.3",
    "stylelint-stylus": "^0.18.0",
    "stylus": "^0.59.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-stylelint": "^4.3.0",
    "vite-plugin-svgr": "^2.4.0"
